{
    "version": 2,
    "builds": [
        {
            "src": "BackEnd/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "FrontEnd",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "FrontEnd/dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "BackEnd/server.js"
        }
    ]
}
