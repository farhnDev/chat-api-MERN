{
    "version": 2,
    "builds": [
      {
        "src": "BackEnd/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "FrontEnd/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "FrontEnd/dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "BackEnd/server.js"
      }
    ]
  }
  